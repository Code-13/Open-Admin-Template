{
  "name": "open-admin-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext ts --ext tsx --ext js",
    "format:prettier": "prettier --write .",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git add . && cz-customizable",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s",
    "init-changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.4.7",
    "antd": "^5.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "engines": {
    "node": ">=14.6.0",
    "pnpm": ">=7"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.6.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-gitmoji-config": "^1.4.7",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-compression": "^0.5.1"
  }
}
